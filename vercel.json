{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "headers": [
      {
          "source": "/(.*)",
          "headers": [
              {
                  "key": "Content-Security-Policy",
                  "value": "upgrade-insecure-requests"
              }
          ]
      }
  ],
  "rewrites": [
      {
          "source": "/(.*)",
          "destination": "/index.html"
      }
  ]
}