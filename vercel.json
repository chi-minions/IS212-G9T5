{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "rewrites": [
      {
          "source": "/api/:path*",
          "destination": "http://3.107.212.99:5001/api/:path*"
      },
      {
          "source": "/(.*)",
          "destination": "/index.html"
      }
  ]
}